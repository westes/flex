	if (yyscanner->yyStartStackOffset >= yyscanner->yyStartStackDepth) {
		size_t newSize;

		yyscanner->yyStartStackDepth += YY_START_STACK_INCR;
		newSize = (size_t) yyscanner->yyStartStackDepth * sizeof(int);

		if (yyscanner->yyStartStack == NULL) {
